{
  "name": "freejointmotion",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://freejointmotion.github.io/",
  "dependencies": {
    "@babel/preset-react": "^7.26.3",
    "@parcel/transformer-react-refresh-babel": "^2.0.0-alpha.3",
    "@react-three/drei": "^9.121.3",
    "@react-three/fiber": "^8.17.12",
    "imagesloaded": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-router": "^7.1.3",
    "swiper": "^11.2.1",
    "three": "^0.172.0"
  },
  "scripts": {
    "start": "parcel src/index.html --open --dist-dir docs",
    "dev": "npm run start",
    "clean": "rm -rf docs/* && rm -rf .parcel-cache",
    "build:parcel": "parcel build src/*.html --no-cache --no-source-maps --public-url ./ --dist-dir docs",
    "build": "npm run clean && npm run build:parcel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@parcel/transformer-glsl": "^2.12.0",
    "@parcel/transformer-sass": "^2.12.0",
    "buffer": "^6.0.3",
    "parcel": "^2.12.0",
    "process": "^0.11.10",
    "svgo": "^3.3.2"
  }
}
